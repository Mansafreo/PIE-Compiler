// Simple example showing improved dictionary validation

dict config = {"server": "localhost", "port": 8080, "debug": 1};

// Safe way to check if a key exists before using it
if(dict_key_exists(config, "server") == 1){
    string server = dict_get_string(config, "server");
    output("Server: ", string);
    output(server, string);
} else {
    output("Server configuration not found", string);
}

// Check for optional configuration
if(dict_key_exists(config, "timeout") == 1){
    int timeout = dict_get_int(config, "timeout");
    output("Timeout: ", string);
    output(timeout, int);
} else {
    output("Using default timeout", string);
}

// Safe access with fallback logic
string log_level;
if(dict_key_exists(config, "log_level") == 1){
    log_level = dict_get_string(config, "log_level");
} else {
    log_level = "info";  // Default value
}
output("Log level: ", string);
output(log_level, string);
